{
  "name": "trekker",
  "version": "1.0.0-alpha01",
  "description": "",
  "author": "iamchristopher",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamchristopher/trekker.git"
  },
  "keywords": [
    "pathfinding"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamchristopher/trekker/issues"
  },
  "homepage": "https://github.com/iamchristopher/trekker#readme",
  "scripts": {
    "build": "BABEL_ENV=build rollup -c",
    "prepare": "npm run build",
    "test": "BABEL_ENV=test nyc mocha --require babel-core/register"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "should": "^13.2.1"
  },
  "dependencies": {
    "heap": "^0.2.6"
  },
  "babel": {
    "env": {
      "build": {
        "presets": [
          "stage-1",
          "es2015-rollup"
        ],
        "plugins": [
          [
            "transform-class-properties",
            {
              "spec": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          "es2015"
        ],
        "plugins": [
          [
            "transform-runtime",
            {
              "helpers": true
            }
          ],
          [
            "transform-class-properties",
            {
              "spec": true
            }
          ]
        ]
      }
    }
  },
  "main": "dist.js",
  "files": [
    "dist.js"
  ]
}
